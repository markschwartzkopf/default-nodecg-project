{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "outDir": "../extension",
    "rootDir": ".",
    "noEmitOnError": true,
    "strict": true,
    "resolveJsonModule": true,
    "typeRoots": ["../types"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}